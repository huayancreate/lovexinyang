<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/bg_table_subtitle" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/ump_header_bg"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:id="@id/i1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:src="@drawable/ic_action_home" />

            <TextView
                android:id="@+id/tv_gengduo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dip"
                android:text="@string/my_more"
                android:textColor="@android:color/white"
                android:textSize="@dimen/head_text_size" />
  
        </LinearLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_margin="3.0dp"
            android:orientation="vertical"
            android:padding="3.0dp" >

            <RelativeLayout
                android:id="@+id/type_layout1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/txt_seting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="8.0dp"
                    android:padding="5.0dp"
                    android:text="设置"
                    android:textColor="@color/black"
                    android:textSize="@dimen/default_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_seting"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout1" >

                <TextView
                    android:id="@+id/txt_share"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="分享设置"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_share"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout3" >

                <TextView
                    android:id="@+id/txt_yaoqing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="清空缓存"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <TextView
                    android:id="@+id/txt_casch_size"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="0K"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_yaoqing"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout4" >

                <TextView
                    android:id="@+id/txt_qita"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="8.0dp"
                    android:padding="5.0dp"
                    android:text="其他"
                    android:textColor="@color/black"
                    android:textSize="@dimen/default_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_qita"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout5" >

                <TextView
                    android:id="@+id/txt_shao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="扫一扫"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_shao"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout6" >

                <TextView
                    android:id="@+id/txt_face_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="@string/feedback"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_face_back"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout7" >

                <TextView
                    android:id="@+id/txt_zhi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="支付帮助"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_zhi"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout8" >

                <TextView
                    android:id="@+id/txt_check"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="检查更新"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_check"
                    android:src="@drawable/divider" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/type_layout10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/type_layout9" >

                <TextView
                    android:id="@+id/txt_about"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5.0dp"
                    android:layout_marginTop="5.0dp"
                    android:padding="5.0dp"
                    android:text="关于爱信阳"
                    android:textColor="@color/text_color_gray"
                    android:textSize="@dimen/set_text_size" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_about"
                    android:src="@drawable/divider" />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

</ScrollView>