用户						UserActionController
	登录					login
	注册					signin		
	获取用户基础信息		getInfo
	变更用户基础信息		updateInfo
	绑定手机				bindPhoneNumber
	绑定手机修改			updateBindPhoneNumber
	短信验证		

login:
提交:
{
	"username": "value",
	"password": "value",
	"opeType": "login",
	"type": "value",（请求来源0web，1mobile）
	"mobile": "value"（请求来源0ios，1android）
}

返回:
{
	"result": "value",(true/false)
	"ID": "value",
	"message": "value" (true:message传递token值, false:message传递错误信息)
}

signin:
提交:
{
	"username": "value", (注册用户名)
	"password": "value", (注册密码)
	"captcha": "value", (注册码 可选)
	"phoneCaptcha": "value" (手机注册码)
	"opeType": "signin",
	"type": "value", (用于统计请求来源 0web, 1mobile)
	"mobile": "value" (用于统计手机来源 0ios, 1android)
}

返回json:
{"success":"true",
	"content":
{
	"result": "value",(true/false)
	"ID": "value",
	"message": "value" (true:message传递token值, false:message传递错误信息)
}
}

getInfo:
提交:
{
	"token":  "value",
	"username": "value",
	"opeType": "getInfo"
}

返回:
{"success":"true",
	"content":{
		 "username": "xuwei",
		  "realname": "许玮",
		  "sex": "男",
	}
}

updateInfo:
提交:
{
	"token":  "value",
	"username": "value",
	"opeType": "updateInfo",
	"sex": "value"
	XXXXX
}
返回json:
{"success":"true",
	"content":{
			"result":"true",
			"message":"更新成功!"
	}
}


bindPhoneNumber:
提交:
{
	"token":  "value",
	"username": "value",
	"opeType": "bindPhoneNumber",
	"phoneCaptcha": "value", (手机验证码)
	"phoneNumber": "value"
}
返回json:
{"success":"true",
	"content":{
			"result":"true",
			"message":"绑定成功!"
	}
}

updateBindPhoneNumber
{
	"token":  "value",
	"username": "value",
	"opeType": "updateBindPhoneNumber",,
	"newPhoneNumber": "value"
	"payPasword": "value" (js md5 加密)
}
返回json:
{"success":"true",
	"content":{
			"result":"true",
			"message":"绑定成功!"
	}
}


